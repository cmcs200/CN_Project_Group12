$ kubectl proxy
# access by localhost: http://localhost:8001/api/v1/namespaces/kubernetes-dashboard/services/https:kubernetes-dashboard:/proxy/#/login

# token
kubectl create serviceaccount yellowtaxi

# secret
kubectl get serviceaccounts yellowtaxi -o yaml

apiVersion: v1
kind: ServiceAccount
metadata:
  creationTimestamp: "2022-04-14T13:22:48Z"
  name: yellowtaxi
  namespace: default
  resourceVersion: "38222"
  uid: 89f9c98a-825c-46b0-94bb-4076d622864b
secrets:
- name: yellowtaxi-token-fwnxd

kubectl get secret yellowtaxi-token-fwnxd -o yaml

--------------------------------------------------------------------------------------------------------------------------------------------------------------------------

kubectl create serviceaccount dashboard -n default

kubectl create clusterrolebinding dashboard-admin -n default --clusterrole=cluster-admin --serviceaccount=default:dashboard

# get token
kubectl get secret $(kubectl get serviceaccount dashboard -o jsonpath="{.secrets[0].name}") -o jsonpath="{.data.token}" | base64 --decode

